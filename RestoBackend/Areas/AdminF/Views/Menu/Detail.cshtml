@model Menu
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/AdminF/Views/Shared/_AdminLayout.cshtml";
}

<h5><img  src="~/assets/img/@Model.Image"/></h5>
<h5>FoodName:@Model.SubTitle</h5>
